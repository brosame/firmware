ca /etc/openvpn/opennet_ugw/opennet-ca_ugws.crt
cert /etc/openvpn/opennet_ugw/on_ugws.crt
key /etc/openvpn/opennet_ugw/on_ugws.key

client
nobind
comp-lzo yes

# sanfte Abmeldung vom Server
explicit-exit-notify 1

dev tap
dev-type tap

persist-key
persist-tun

ns-cert-type server
script-security 2 execve

up /etc/openvpn/opennet_ugw/opennet_ugw_up.sh
down /etc/openvpn/opennet_ugw/opennet_ugw_down.sh

